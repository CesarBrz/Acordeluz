  while (Serial.available() > 0)
  {
    byte Entrada = Serial.read();

    if (Entrada == 67 || Entrada == 99) {FastLED.clear();}//limpa painel

    if (Entrada == 48) {Entrada == 58;}//se zero, soma 10 para funcionar no algoritmo abaixo

    //ativa e desativa algoritmos 1 a 10 
    if (Entrada >= 49 && Entrada <= 58)
    {
      if (FlagAlgoritmoAtivo[Entrada-48] == 1)
      {
        FlagAlgoritmoAtivo[Entrada-48] = 0;
        Serial.print ("Algoritmo Desativado - "); Serial.println(Entrada-48);
      }
      else
      {
        FlagAlgoritmoAtivo[Entrada-48] = 1;
        Serial.print ("Algoritmo Ativado - "); Serial.println(Entrada-48);
      }
    }

    // letra E/e - Esmaecimento escorre
