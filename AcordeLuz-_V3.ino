void GeraDistanciaCentro(uint8_t channel, uint8_t note, uint8_t velocity, uint16_t timestamp)
{
  //Algoritmo de distância do centro

  byte Multiplicador = 2;

  byte NumLinhas = 12; //numero de linhas controladas por essa placa
  //byte LedsLinha = 75;
  byte OffsetNota = 24; // C0 = 0
  byte OffsetPxIni = 6;

  if (velocity == 0) {
    return; //se velocity for zero ignora a nota
  }

  //trunca notas para as 75 colunas do painel
  if (note < OffsetNota) {
    note = OffsetNota;
  }
  if (note > 101) {
    note = 101;
  }

  if (velocity > 30) {
    velocity = 30; //limita velocity
  }
  if (velocity < 10) {
    velocity = 10;
  }

  int CvtNota = note - OffsetNota; //placeholder caso tenha que converter notas de alguma forma

  //OitavaNota define quão long do centro, GrauNota define qual linha
  byte GrauNota = CvtNota % 12; // C = 0, C# = 1, D = 2, D# = 3... B = 11
  byte OitavaNota = int(CvtNota / 12.0);

  //Se for a metade de baixo, inverte a linha para espelhar verticalmente
  if (ModuloESP == 1) {
    GrauNota = 11 - GrauNota;
  }

  //CORRIGIR: Está funcionando com uma oitava a menos do que deveria

  //Calcula os pixeis pivôs para a nota a ser renderizada
  //pixel inicial
  int PxInicial = OffsetPxIni + ((7 - OitavaNota) * 4) + (GrauNota * LedsPorLinha);
  //pixel inicial do espelho horizontal
  int PxEspelhoHor = (38) + (OitavaNota * 4) + (GrauNota * LedsPorLinha);

  int PxCentral = OffsetPxIni + 28 + (GrauNota * LedsPorLinha); //sempre inicia na posição da oitava zero

  byte ON;//armazena se pixel acende ou não
  byte Probabilidade = 3;// 1/Probabilidade chances de pixel não acender

  //acende pixels do centro com intensidade reduzida
  //uma vez que sempre serão os 8 pixels centrais, dispensa calculo de espelhamento
  for (int i = 0; i <= 7; i++)
  {
    ON = random(Probabilidade); if (ON > 0) {
      ON = 1;
    }
    leds[Remap(PxCentral + i)].r += random(velocity) * ON * Multiplicador;
    leds[Remap(PxCentral + i)].g += random(velocity) * ON * Multiplicador;
    leds[Remap(PxCentral + i)].b += random(velocity) * ON * Multiplicador;

    //Acende linha anterior (duplicata)
    ON = random(Probabilidade); if (ON > 0) {
      ON = 1;
    }
    leds[Remap(PxCentral + i - LedsPorLinha)].r += random(velocity) * ON * Multiplicador;
    leds[Remap(PxCentral + i - LedsPorLinha)].g += random(velocity) * ON * Multiplicador;
    leds[Remap(PxCentral + i - LedsPorLinha)].b += random(velocity) * ON * Multiplicador;
  }

  //Acende pixels laterais de acordo com nota e oitava
  //Acende respectivos espelhamentos e duplicatas
  for (int i = 0 ; i <= 5; i++)//aumentei para 6 leds por nota
  {
    //alterar para velocity influenciar a cor e adicionar randomicidade

    //pixels originais
    ON = random(Probabilidade); if (ON > 0) {
      ON = 1; //chance de led desligado
    }
    leds[Remap(PxInicial + i)].r += random(velocity) * (((14 - OitavaNota) / 7.0) / 2) * ON * Multiplicador;
    leds[Remap(PxInicial + i)].g += random(velocity) * (((14 - OitavaNota) / 7.0) / 2) * ON * Multiplicador;
    leds[Remap(PxInicial + i)].b += random(velocity) * (((14 - OitavaNota) / 7.0) / 2) * ON * Multiplicador;

    //DUPLICATA LINHA ANTERIOR
    ON = random(Probabilidade); if (ON > 0) {
      ON = 1;
    }
    leds[Remap(PxInicial + i - LedsPorLinha)].r += random(velocity) * (((14 - OitavaNota) / 7.0) / 2) * ON * Multiplicador;
    leds[Remap(PxInicial + i - LedsPorLinha)].g += random(velocity) * (((14 - OitavaNota) / 7.0) / 2) * ON * Multiplicador;
    leds[Remap(PxInicial + i - LedsPorLinha)].b += random(velocity) * (((14 - OitavaNota) / 7.0) / 2) * ON * Multiplicador;
    //------------------------------------------------------------
    //pixels espelhados
    ON = random(Probabilidade); if (ON > 0) {
      ON = 1;
    }
    leds[Remap(PxEspelhoHor + i)].r += random(velocity) * (((14 - OitavaNota) / 7.0) / 2) * ON * Multiplicador;
    leds[Remap(PxEspelhoHor + i)].g += random(velocity) * (((14 - OitavaNota) / 7.0) / 2) * ON * Multiplicador;
    leds[Remap(PxEspelhoHor + i)].b += random(velocity) * (((14 - OitavaNota) / 7.0) / 2) * ON * Multiplicador;

    //DUPLICATA LINHA POSTERIOR
    ON = random(Probabilidade); if (ON > 0) {
      ON = 1;
    }
    leds[Remap(PxEspelhoHor + i - LedsPorLinha)].r += random(velocity) * (((14 - OitavaNota) / 7.0) / 2) * ON * Multiplicador;
    leds[Remap(PxEspelhoHor + i - LedsPorLinha)].g += random(velocity) * (((14 - OitavaNota) / 7.0) / 2) * ON * Multiplicador;
    leds[Remap(PxEspelhoHor + i - LedsPorLinha)].b += random(velocity) * (((14 - OitavaNota) / 7.0) / 2) * ON * Multiplicador;
  }

}

void GeraAneis (uint8_t channel, uint8_t note, uint8_t velocity, uint16_t timestamp, byte ModoCor)
{

  //calcula grau da nota, oitava e garante nota positiva
  int Nota = note-36;
  while (Nota <0) 
  {Nota += 12;}// se nota cair abaixo de zero, soma uma oitava

  int Oitava = Nota/12.0;
  Nota = Nota%12;

  //garante oitava no range adequado
  if (Oitava > 5) {Oitava = 5;}
  if (Oitava < 0) {Oitava = 0;}

  //calcula o offset com base na oitava
  int Offset = 10*Oitava;

  Offset+=2; //arrasta 2 pixels pra dentro
  if (Offset < 2) {Offset = 2;}
  if (Offset > 52) {Offset = 52;}
  
  if (ModoCor == 0)
  {
    
    DesenhaAneisSolido(Nota, Offset, 22,22);
  }
  else
  {
    DesenhaAneisRandom(Nota, Offset, 22,22);
  }
  
}

