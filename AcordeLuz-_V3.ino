  byte Lento = 8;// corte para esmaecimento mais lento
  byte Velocidade = 100;
  
  if ( (FrameAtual - UltimoFrame[0]) < Velocidade) {
    return; //se não deu o tempo mínimo aborta
  }

  for (int i = 0; i < NUM_LEDS; i++)
  {
    if (leds[i].r - Limiar > Lento) {
      leds[i].r = int(leds[i].r - Atenuacao);
    }
    else if (leds[i].r - Limiar > 0 && leds[i].r - Limiar <= Lento) {
      leds[i].r = int(leds[i].r - Atenuacao/2.0);
    }
    else {
      leds[i].r = 0;
    }

    if (leds[i].g - Limiar > 0) {
      leds[i].g = int(leds[i].g - Atenuacao);
    }
    else if (leds[i].g - Limiar > 0 && leds[i].g - Limiar <= Lento) {
      leds[i].g = int(leds[i].g - Atenuacao/2.0);
    }
    else {
      leds[i].g = 0;
    }

    if (leds[i].b - Limiar > 0) {
      leds[i].b = int(leds[i].b - Atenuacao);
    }
    else if (leds[i].b - Limiar > 0 && leds[i].b - Limiar <= Lento) {
      leds[i].b = int(leds[i].b - Atenuacao/2.0);
    }
    else {
      leds[i].b = 0;
    }
  }

  UltimoFrame[0] = FrameAtual;

}

void EscorreNotas ()
{
  //Funcao de esmaecimento 2
  
  //Atenua bastante linha 0.
  //Arrasta todas as linhas para baixo excluindo a última
  //repete

  long FrameAtual = millis();
  float Atenuacao = 0.95 ;
  byte Limiar = 1;
  byte NumLinhas = 12;
  byte Velocidade = 40;

  if ( (FrameAtual - UltimoFrame[1]) < Velocidade) {
    return; //se não deu o tempo mínimo aborta
  }

  if (ModuloESP == 0) //metade de cima escorre para baixo
  {
    for (int k = 0; k < LedsPorLinha; k++)
    {
      leds[k].r = leds[k].r * Atenuacao;//atenua linha da borda
      leds[k].g = leds[k].g * Atenuacao;
      leds[k].b = leds[k].b * Atenuacao;
      
    }
    
    for (int i = (NumLinhas-1); i > 0; i --)
    {
      for (int j = 0; j < LedsPorLinha; j++)
      {
        
        leds[Remap(j + (i*LedsPorLinha))].r = (((leds[Remap( ( j + (i*LedsPorLinha) ) - LedsPorLinha )].r) *Atenuacao) + leds[Remap(j + (i*LedsPorLinha))].r)/2.0; //copia da linha anterior e atenua
        leds[Remap(j + (i*LedsPorLinha))].g = (((leds[Remap( ( j + (i*LedsPorLinha) ) - LedsPorLinha )].g) *Atenuacao) + leds[Remap(j + (i*LedsPorLinha))].g)/2.0; //copia da linha anterior e atenua
        leds[Remap(j + (i*LedsPorLinha))].b = (((leds[Remap( ( j + (i*LedsPorLinha) ) - LedsPorLinha )].b) *Atenuacao) + leds[Remap(j + (i*LedsPorLinha))].b)/2.0; //copia da linha anterior e atenua
        
      }
    }
  }

  if (ModuloESP == 1) //metade de baixo escorre para cima
  {
    for (int k = (NUM_LEDS-1); k >= (NUM_LEDS-1-LedsPorLinha) ; k--)
    {
      leds[k].r = leds[k].r * Atenuacao;//atenua linha da borda
      leds[k].g = leds[k].g * Atenuacao;
      leds[k].b = leds[k].b * Atenuacao; 
    }
    
    for (int i = 0; i < (NumLinhas-1); i ++)
    {
      for (int j = 0; j < LedsPorLinha; j++)
      {
        
        leds[Remap(j + (i*LedsPorLinha))].r = (((leds[Remap( ( j + (i*LedsPorLinha) ) + LedsPorLinha )].r) *Atenuacao) + leds[Remap(j + (i*LedsPorLinha))].r)/2.0; //copia da linha anterior e atenua
        leds[Remap(j + (i*LedsPorLinha))].g = (((leds[Remap( ( j + (i*LedsPorLinha) ) + LedsPorLinha )].g) *Atenuacao) + leds[Remap(j + (i*LedsPorLinha))].g)/2.0; //copia da linha anterior e atenua
        leds[Remap(j + (i*LedsPorLinha))].b = (((leds[Remap( ( j + (i*LedsPorLinha) ) + LedsPorLinha )].b) *Atenuacao) + leds[Remap(j + (i*LedsPorLinha))].b)/2.0; //copia da linha anterior e atenua
      
      }
    }
  }
    
  UltimoFrame[1] = FrameAtual;
}

