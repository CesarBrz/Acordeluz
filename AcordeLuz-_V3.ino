//FUNCOES DE CALCULO E REMAPEAMENTO
//------------------------------------------------------------------------------------

int Remap (int Entrada) //remapeia o numero linear do led para o zig zag da fita led
{
  int Saida;
  //int LedsLinha = 75;
  int NumLinhas = int(Entrada / LedsPorLinha);
  bool Impar = false;

  //se a coluna for par, retorna o mesmo led de entrada
  //se a coluna for impar, inverte a posição do led na linha para considerar o zigzag da fita led

  if (Entrada >= 75 && Entrada <= 149 || Entrada >= 225 && Entrada <= 299 || Entrada >= 375 && Entrada <= 449 || Entrada >= 525 && Entrada <= 599 || Entrada >= 675 && Entrada <= 749 || Entrada >= 825 && Entrada <= 899 || Entrada >= 975 && Entrada <= 1049 || Entrada >= 1125 && Entrada <= 1199 || Entrada >= 1275 && Entrada <= 1349 || Entrada >= 1425 && Entrada <= 1499 || Entrada >= 1575 && Entrada <= 1649 || Entrada >= 1725 && Entrada <= 1799)
  {
    Impar = true; //linha impar
  }

  if (Impar == false)
  {
    Saida = Entrada;
  }
  else
  {
    Saida = (((NumLinhas + 1) * LedsPorLinha) - (Entrada % LedsPorLinha)) - 1;
  }
  //garante saída dentro do range adequado
  if (Saida < 0) {
    Saida = 0;
  }
  if (Saida > NUM_LEDS - 1) {
    Saida = NUM_LEDS - 1;
  }
  return Saida;
}

